#!/usr/bin/env python3
# author: Jan Hybs

import sys


try:
    import cihpc.core
except ImportError as e:
    print('sys.path:\n  ' + '\n  '.join(['%2d) %s' % (i + 1, sys.path[i]) for i in range(len(sys.path))]))
    sys.exit(1)


print('cihpc-core v%s' % cihpc.__version__)
sys.exit(
    cihpc.core.main()
)
